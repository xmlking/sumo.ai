{
  "name": "bot-ui",
  "version": "1.0.0",
  "private": true,
  "description": "BOT UI",
  "style": "dist/css/style.css",
  "sass": "src/scss/style.scss",
  "main": "dist/script.js",
  "dependencies": {
    "api-ai-javascript": "^2.0.0-beta.21",
    "bootstrap": "^4.0.0-beta.2",
    "bootstrap-material-design": "^4.0.0-beta.4",
    "jquery": "^3.2.1"
  },
  "devDependencies": {
    "@types/core-js": "^0.9.43",
    "@types/jquery": "^3.2.15",
    "@types/node": "^8.0.24",
    "awesome-typescript-loader": "^3.2.3",
    "copy-webpack-plugin": "^4.6.0",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.7",
    "ejs-loader": "^0.5.0",
    "expose-loader": "^0.7.3",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^5.5.0",
    "node-sass": "^4.5.3",
    "sass-loader": "^6.0.6",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.19.0",
    "tslint": "^5.8.0",
    "tslint-loader": "^3.5.3",
    "typescript": "^2.5.3",
    "webpack": "^5.75.0",
    "webpack-dev-server": "^2.9.3"
  },
  "scripts": {
    "start": "npm run server:dev",
    "server": "npm run server:dev",
    "server:dev": "webpack-dev-server --config ./webpack.config.js --hot --inline --progress --watch --open",
    "server:prod": "cross-env NODE_ENV=production webpack-dev-server --config ./webpack.config.js --port 3000 --host 0.0.0.0 --hot --inline --progress --profile --watch --open --content-base dist/",
    "build": "npm run build:dev",
    "build:dev": "webpack --config ./webpack.config.js --progress --profile --color --display-error-details --display-cached",
    "build:prod": "cross-env NODE_ENV=production webpack --config ./webpack.config.js  --progress --profile --color --display-error-details --display-cached --bail",
    "clean": "rimraf -- dist",
    "test": "karma start"
  },
  "author": "Sumanth Chinthagunta",
  "license": "MIT"
}
